{
  "expo": {
    "name": "DearRequest",
    "slug": "DearRequest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/custom-icon.png",
    "splash": {
      "image": "./assets/custom-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e20b0b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dealmeidagabriel.dearrequest"
    },
    "android": {
      "package": "com.dealmeidagabriel.dearrequest",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
